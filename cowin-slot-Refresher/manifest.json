{
  "manifest_version": 2,
  "name": "Cowin Web finder",
  "description": "To available vaccine slots",
  "version": "1.1.0",
  "author":"thava",
  "browser_action":{
    "default_icon":"icon.png",
    "default_popup":"popup.html"
  },
  "background":{
    "scripts":["jquery.min.js","popup.js"],
    "pages":["popup.html"]
  },
  "permissions": [
    "activeTab","storage","notifications","activeTab","https://www.cowin.gov.in/home","https://selfregistration.cowin.gov.in/appointment"
  ],
  "web_accessible_resources":[
    "note.mp3"
  ]
}
